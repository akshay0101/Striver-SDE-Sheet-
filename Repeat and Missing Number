unordered_map<int, bool> numberMap;
 for(int i : arr)
    {
        if (numberMap.find(i) ==
            numberMap.end())
     {
            numberMap[i] = true;            // using maps  T-O(N)  S-O(N)
      }
        else
        {
            cout << "Repeating = " << i;
        }
    }
    cout << endl;
     
    for(int i = 1; i <= n; i++)
    {
        if (numberMap.find(i) ==
            numberMap.end())
        {
            cout << "Missing = " << i;
         }
 }
 
 
    
    
